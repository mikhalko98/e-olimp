#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    if(n<2) cout<<0;
    else {
        int mas[n];
        int chek0=0;
        int chek1=0;
        for(int i=0 ; i<n ; i++){
            cin>>mas[i];
            if(mas[i]==0)chek0++;
            else chek1++;
        }
        if(chek0<chek1) cout<<chek0;
        if(chek0>chek1) cout<<chek1;
        if(chek0==chek1) cout<<chek1;


        }
    return 0;
}
